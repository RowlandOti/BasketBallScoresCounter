    <!-- Base application theme. -->
    <style name="AppBaseThemeNoActionBar.Dark" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/apptheme_primary_grey_light</item>
        <item name="colorPrimaryDark">@color/apptheme_primary_grey</item>
        <item name="colorAccent">@color/apptheme_accent_teal</item>
